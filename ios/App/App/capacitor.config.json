{
	"appId": "com.xcapit.iosapp",
	"appName": "Xcapit",
	"bundledWebRuntime": false,
	"npmClient": "yarn",
	"webDir": "www",
	"backgroundColor": "#ffffffff",
	"linuxAndroidStudioPath": "/usr/local/android-studio/bin/studio.sh",
	"plugins": {
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"GoogleAuth": {
			"scopes": [
				"https://www.googleapis.com/auth/drive.file",
				"https://www.googleapis.com/auth/drive.appdata"
			],
			"serverClientId": "840751586681-6mrinfnm1o42lge5pdbb64l5h57mjtqv.apps.googleusercontent.com
",
			"forceCodeForRefreshToken": true
		},
		"SplashScreen": {
			"launchAutoHide": true,
			"launchShowDuration": 2000
		},
		"LocalNotifications": {
			"smallIcon": "ic_stat_icon_local",
			"iconColor": "#1c2d5e"
		}
	}
}
