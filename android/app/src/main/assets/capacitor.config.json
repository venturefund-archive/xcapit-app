{
	"appId": "com.xcapit.app",
	"appName": "xcapit",
	"bundledWebRuntime": false,
	"npmClient": "yarn",
	"webDir": "www",
	"backgroundColor": "#ffffffff",
	"linuxAndroidStudioPath": "/usr/local/android-studio/bin/studio.sh",
	"plugins": {
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"GoogleAuth": {
			"scopes": [
				"https://www.googleapis.com/auth/drive.file",
				"https://www.googleapis.com/auth/drive.appdata"
			],
			"serverClientId": "840751586681-35nflvl2e8e8tn0mb6uin9bgrptjgkf6.apps.googleusercontent.com",
			"forceCodeForRefreshToken": true
		},
		"SplashScreen": {
			"launchAutoHide": true,
			"launchShowDuration": 2000
		},
		"LocalNotifications": {
			"smallIcon": "ic_stat_icon_local",
			"iconColor": "#1c2d5e"
		}
	}
}
