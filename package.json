{
  "name": "app",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "build:prod:xcapit:android": "node set-capacitor-config.js android && npm run build:prod:xcapit && npx jetify && npx cap sync android && npx cap open android",
    "build:prod:xcapit:ios": "node set-capacitor-config.js ios && npm run build:prod:xcapit && npx cap sync ios && npx cap open ios",
    "set-brand:xcapit": "node set-brand.js xcapit",
    "set-brand:wcs": "node set-brand.js wcs",
    "build:prod:xcapit": "node set-brand.js xcapit && ng build --prod",
    "build:prod:wcs": "node set-brand.js wcs && ng build --prod",
    "build:prod:pwa:xcapit": "node set-brand.js xcapit && ng build --prod --serviceWorker=true",
    "build:prod:pwa:wcs": "node set-brand.js wcs && ng build --prod --serviceWorker=true",
    "build:prod:local:xcapit": "node set-brand.js xcapit && ng build --prod && git checkout -- src/index.html && git checkout -- src/manifest.webmanifest && git checkout -- capacitor.config.json",
    "build:prod:local:wcs": "node set-brand.js wcs && ng build --prod && git checkout -- src/index.html && git checkout -- src/manifest.webmanifest && git checkout -- capacitor.config.json",
    "build:prod:local:pwa:xcapit": "node set-brand.js xcapit && ng build --prod --serviceWorker=true && git checkout -- src/index.html && git checkout -- src/manifest.webmanifest && git checkout -- capacitor.config.json",
    "build:prod:local:pwa:wcs": "node set-brand.js wcs && ng build --prod --serviceWorker=true && git checkout -- src/index.html && git checkout -- src/manifest.webmanifest && git checkout -- capacitor.config.json",
    "build:test:app": "ionic build && npx cap copy && npx cap open android &",
    "ng": "ng",
    "start": "ionic serve",
    "start:xcapit": "node set-brand.js xcapit && ionic serve",
    "start:wcs": "node set-brand.js wcs && ionic serve",
    "build": "ng build",
    "test": "ng test",
    "prepare": "husky install",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^10.0.11",
    "@angular/compiler": "^10.0.11",
    "@angular/core": "^10.0.11",
    "@angular/forms": "^10.0.11",
    "@angular/localize": "^10.0.11",
    "@angular/platform-browser": "^10.0.11",
    "@angular/platform-browser-dynamic": "^10.0.11",
    "@angular/pwa": "^0.1000.6",
    "@angular/router": "^10.0.11",
    "@angular/service-worker": "^10.0.11",
    "@auth0/angular-jwt": "^5.0.1",
    "@capacitor-community/barcode-scanner": "^1.2.1",
    "@capacitor-community/firebase-analytics": "^0.3.3",
    "@capacitor-community/native-market": "^0.1.4",
    "@capacitor/android": "^2.4.1",
    "@capacitor/core": "^2.4.0",
    "@capacitor/ios": "^2.4.1",
    "@codetrix-studio/capacitor-google-auth": "^2.1.3",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/file-opener": "^5.31.1",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.0.1",
    "@ionic/storage": "^2.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "chart.js": "^2.8.0",
    "cordova": "^10.0.0",
    "cordova-plugin-file-opener2": "^3.0.5",
    "core-js": "^3.6.5",
    "ethers": "^5.4.3",
    "firebase": "^7.21.0",
    "ionic": "^5.4.16",
    "ionicons": "^5.5.0",
    "jetifier": "^1.6.6",
    "lightweight-charts": "^3.2.0",
    "lodash": "^4.17.21",
    "qrcode": "^1.4.2",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.1.0",
    "@angular-devkit/architect": "~0.1000.6",
    "@angular-devkit/build-angular": "^0.1000.6",
    "@angular-devkit/core": "~10.0.6",
    "@angular-devkit/schematics": "~10.0.6",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "^10.0.11",
    "@angular/language-service": "^10.0.11",
    "@capacitor/cli": "^2.4.1",
    "@ionic/angular-toolkit": "^2.3.1",
    "@types/chart.js": "^2.8.7",
    "@types/googlemaps": "^3.37.7",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.17.3",
    "codelyzer": "^6.0.0",
    "husky": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "^3.9.7"
  },
  "resolutions": {
    "@codetrix-studio/capacitor-google-auth/@capacitor/core": "2.4.8"
  },
  "postinstall": "jetifier",
  "description": "An Ionic project"
}
