  "homepage": "https://ionicframework.com/",
  "scripts": {
    "build:prod:xcapit:android": "node set-capacitor-config.js android && npm run build:prod:xcapit && npx jetify && npx cap sync android && npx cap open android",
    "build:prod:xcapit:ios": "node set-capacitor-config.js ios && npm run build:prod:xcapit && npx cap sync ios && npx cap open ios",
    "set-brand:xcapit": "node set-brand.js xcapit",
    "set-brand:wcs": "node set-brand.js wcs",
    "build:prod:xcapit": "node set-brand.js xcapit && ng build --configuration production",
    "build:prod:wcs": "node set-brand.js wcs && ng build --configuration production",
    "build:prod:pwa:xcapit": "node set-brand.js xcapit && ng build --configuration production --serviceWorker=true",
    "build:prod:pwa:wcs": "node set-brand.js wcs && ng build --configuration production --serviceWorker=true",
    "build:prod:local:xcapit": "node set-brand.js xcapit && ng build --configuration production && git checkout -- src/index.html && git checkout -- src/manifest.webmanifest && git checkout -- capacitor.config.json",
    "build:prod:local:wcs": "node set-brand.js wcs && ng build --configuration production && git checkout -- src/index.html && git checkout -- src/manifest.webmanifest && git checkout -- capacitor.config.json",
    "build:prod:local:pwa:xcapit": "node set-brand.js xcapit && ng build --configuration production --serviceWorker=true && git checkout -- src/index.html && git checkout -- src/manifest.webmanifest && git checkout -- capacitor.config.json",
    "build:prod:local:pwa:wcs": "node set-brand.js wcs && ng build --configuration production --serviceWorker=true && git checkout -- src/index.html && git checkout -- src/manifest.webmanifest && git checkout -- capacitor.config.json",
    "build:test:app": "ionic build && npx cap copy && npx cap open android &",
    "ng": "ng",
    "start": "ionic serve",
    "start:xcapit": "node set-brand.js xcapit && ionic serve",
    "start:wcs": "node set-brand.js wcs && ionic serve",
    "build": "ng build",
    "test": "ng test",
    "test-ci": "ng test --watch=false --karma-config karma-pipeline.conf.js --code-coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prepare": "husky install",
    "format:fix": "pretty-quick --staged",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "patch:walletconnect": "sh ./walletconnect-patch.sh | echo 'walletconnect patched.'"
  },
